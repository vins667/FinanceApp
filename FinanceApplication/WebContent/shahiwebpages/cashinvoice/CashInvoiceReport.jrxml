	<queryString>
		<![CDATA[SELECT SL_NO,COMP_NAME, COMP_ADDRESS, COMP_GSTN, BILL_NO, BILL_DATE, SUPP_NAME, SUPP_ADDRESS, SAECAR, CKTX40, PRODUCT_DESC, SUBSTR(HSN_CODE,0,8) HSN_CODE, PRODUCT_QUANTITY, UOM, VOUCHER_DESC, PRODUCT_AMOUNT,OPVRNO,SACSCD,
NVL(SUM(CASE WHEN COST_ELEMENT='16652' OR COST_ELEMENT='26952' THEN GST_PER END),0) IGST_PER,
NVL(SUM(CASE WHEN COST_ELEMENT='16652' OR COST_ELEMENT='26952' THEN  AMT END),0) IGST_VALUE,
NVL(SUM(CASE WHEN COST_ELEMENT='16651' OR COST_ELEMENT='26951' THEN GST_PER END),0) SGST_PER,
NVL(SUM(CASE WHEN COST_ELEMENT='16651' OR COST_ELEMENT='26951' THEN  AMT END),0) SGST_VALUE,
NVL(SUM(CASE WHEN COST_ELEMENT='16650' OR COST_ELEMENT='26950' THEN GST_PER END),0) CGST_PER,
NVL(SUM(CASE WHEN COST_ELEMENT='16650' OR COST_ELEMENT='26950' THEN  AMT END),0) CGST_VALUE
FROM(
 SELECT C.SL_NO,B.WHSUNM COMP_NAME,NVL(B.WHADR1||' ','') || NVL(B.WHADR2||' ','') || NVL(B.WHADR3||' ','') || NVL(B.WHADR4,'') COMP_ADDRESS,
 B.GSTN COMP_GSTN, A.BILL_NO, TO_CHAR(A.BILL_DATE,'dd-Mon-yyyy') BILL_DATE,A.SASUNM SUPP_NAME,
 NVL(TRIM(A.SAADR1)||' ','') || NVL(TRIM(A.SAADR2)||' ','') || NVL(TRIM(A.SAADR3)||' ','') || NVL(TRIM(A.SAADR4)||' ','') || NVL(TRIM(A.SATOWN)||' ','') || NVL(TRIM(A.SAECAR)||' ','') || NVL(TRIM(A.SACSCD)||' ','') SUPP_ADDRESS,A.SAECAR,F.CKTX40,
 D.PRODUCT_DESC,C.HSN_CODE,C.PRODUCT_QUANTITY, C.UOM, C.VOUCHER_DESC,G.OPVRNO,A.SACSCD, C.PRODUCT_AMOUNT,E.GST_PER,E.AMT,E.COST_ELEMENT
 FROM M4_INVOICE_MASTER A,M4_WHLO_MASTER B,M4_INVOICE_DETAIL C, M4_BILL_PRODUCT_MASTER D, M4_INVOICE_BREAKUP_DETAIL E, MOVEX.CSYSTS@AMS F, PRODBI.OCUSAD@AMS G
 where A.BILL_WHLO=B.M4WHLO AND A.SL_NO=C.BILL_SL_NO AND C.PRODUCT_SL_NO=D.SL_NO AND
 C.BILL_SL_NO=E.BILL_SL_NO AND C.SL_NO=E.LINE_SL_NO AND F.CKCONO='111' AND A.SAECAR=F.CKECAR AND
 A.SACSCD=F.CKCSCD AND G.OPCONO='111' AND A.SUPPLIER_CODE=G.OPCUNO AND G.OPADRT='1' AND
 A.SAADID=G.OPADID AND A.SL_NO=$P{SL_NO}
 )
 GROUP BY SL_NO,COMP_NAME, COMP_ADDRESS, COMP_GSTN, BILL_NO, BILL_DATE, SUPP_NAME, SUPP_ADDRESS, SAECAR, PRODUCT_DESC, HSN_CODE,
 PRODUCT_QUANTITY, UOM, VOUCHER_DESC, PRODUCT_AMOUNT, CKTX40, OPVRNO, SACSCD
 ORDER BY SL_NO]]>
